#--------------------------------------------------------------------------------------------------------------
#
# Begin mvn make targets
#
#--------------------------------------------------------------------------------------------------------------

#--------------------------------------------------------------------------------------------------------------
# mvn test
#--------------------------------------------------------------------------------------------------------------
mvn/test:
	mvn test

#--------------------------------------------------------------------------------------------------------------
# mvn clean
#--------------------------------------------------------------------------------------------------------------
mvn/clean:
	mvn clean

#--------------------------------------------------------------------------------------------------------------
# mvn exec
#--------------------------------------------------------------------------------------------------------------
mvn/exec: mvn/clean
	mvn compile exec:java

#--------------------------------------------------------------------------------------------------------------
# mvn package
#--------------------------------------------------------------------------------------------------------------
mvn/package: mvn/clean
	mvn package

#--------------------------------------------------------------------------------------------------------------
#
# End mvn make targets
#
#--------------------------------------------------------------------------------------------------------------